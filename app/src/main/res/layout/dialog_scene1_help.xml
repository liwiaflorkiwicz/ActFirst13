        android:clickable="false"
        android:contentDescription="Check blood circulation"
        app:backgroundTint="#FF4081"
        app:layout_constraintEnd_toEndOf="@+id/actionButtonCheckCons"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/actionButtonCheckCons"
        app:layout_constraintTop_toBottomOf="@+id/actionButtonCheckCons"
        app:srcCompat="@drawable/baseline_face_24" />

    <TextView
        android:id="@+id/textForHelp8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="check consciousness"
        app:layout_constraintBottom_toBottomOf="@+id/actionButtonCheckCons"
        app:layout_constraintEnd_toStartOf="@+id/actionButtonCheckCons"
        app:layout_constraintHorizontal_bias="0.858"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/actionButtonCheckCons"
        app:layout_constraintVertical_bias="0.513" />

