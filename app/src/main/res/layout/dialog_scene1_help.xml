        app:layout_constraintVertical_bias="0.513" />

    <TextView
        android:id="@+id/textForHelp4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="do compressions"
        app:layout_constraintBottom_toBottomOf="@+id/actionButtonCompression"
