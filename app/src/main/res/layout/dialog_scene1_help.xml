        android:layout_marginEnd="16dp"
        android:text="check blood flow"
        app:layout_constraintBottom_toBottomOf="@+id/actionButtonBlood"
        app:layout_constraintEnd_toStartOf="@+id/actionButtonBlood"
        app:layout_constraintHorizontal_bias="0.96"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/actionButtonBlood"
        app:layout_constraintVertical_bias="0.54" />

    <TextView
        android:id="@+id/textForHelp2"
        android:layout_width="wrap_content"
